#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    string s;
	    cin>>s;
	    int one=0,zero=0;
	    for(int i=0;i<n;i++){
	        if(s[i]=='1')
	        one++;
	        else zero++;
	    }
	    if(n%2==0){
	        if(one%2 && zero%2)
	        cout<<"No"<<endl;
	        else
	        cout<<"Yes"<<endl;
	    }
	    else{
	        cout<<"Yes"<<endl;
	    }
	}
	return 0;
}
