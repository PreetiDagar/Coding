#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int x,y;
        cin>>x>>y;
        if(x%2)
        x=x/2+1;
        else
        x=x/2;
        if(y>=x)
        cout<<"Yes"<<endl;
      
        else
        cout<<"No"<<endl;
    }

	return 0;
}
