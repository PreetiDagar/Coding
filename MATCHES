#include <bits/stdc++.h>
using namespace std;
int main() {int t,a,b;
cin>>t;
while(t--){cin>>a>>b;
int out=0;
    int ans=a+b;
    string num= to_string(ans);
    int l=num.length();
    for(int i=0;i<l;i++){char z=num[i];
    if(z=='0'||z=='6'||z=='9')out+=6;
    else if(z=='1')out+=2;
    else if(z=='8')out+=7;
    else if (z=='7')out+=3;
    else if(z=='4')out+=4;
    else out+=5;}
    cout<<out<<endl;
    
}
	
	return 0;
}
