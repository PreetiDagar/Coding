#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    float a,b,x,y;
	    cin>>a>>b>>x>>y;
	    float w=a/x;
	    float z=b/y;
	    if(z>w){
	        cout<<"Chef"<<endl;
	    }
	    else if(w>z)
	    cout<<"Chefina"<<endl;
	    else
	    cout<<"Both"<<endl;
	}
	return 0;
}
