#include <iostream>
using namespace std;

int main() {
	int T;
	cin>>T;
	
	    while(T-->0)
    {
        string x,y,ans;
        cin>>x>>y;
        for(int i=0;i<5;i++)
        {
            if(x[i]==y[i])
            {
                ans+='G';
            }
            else
            {
                ans+='B';
            }
        }
        cout<<ans<<endl;
    
	}
	return 0;
}
