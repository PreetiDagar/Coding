#include <iostream>
using namespace std;

int main() {
    int t;
    cin >>t;
    while(t--){
        int n, k;
        cin >> n >> k;
    int H[n];
    for(int i = 0; i < n; i++){
        cin >> H[i];
    }
    int temp = 0;
    for(int i = 0; i < n; i++){
        if(H[i] > k){
            temp++;
        }
    }
    cout << temp << endl;
    }

	return 0;
}
